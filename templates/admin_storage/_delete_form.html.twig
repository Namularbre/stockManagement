<form class="mb-3" method="post" action="{{ path('app_admin_storage_delete', {'id': storage.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ storage.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
